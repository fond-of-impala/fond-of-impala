<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="Customer">
        <property name="hasAvailabilityRestrictions" type="boolean"/>
    </transfer>

    <transfer name="ConditionalAvailabilityPeriodPageSearch">
        <property name="fkConditionalAvailability" type="int" />
        <property name="originalStartAt" type="string" />
        <property name="startAt" type="string" />
        <property name="endAt" type="string" />
        <property name="storeName" type="string" />

        <property name="conditionalAvailabilityPeriodKey" type="string" />

        <property name="data" type="array" singular="data" />
        <property name="structuredData" type="string" />
    </transfer>

    <transfer name="SynchronizationData">
        <property name="key" type="string"/>
        <property name="data" type="string"/>
    </transfer>
</transfers>
