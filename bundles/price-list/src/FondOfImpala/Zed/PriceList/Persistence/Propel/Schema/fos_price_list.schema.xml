<?xml version="1.0"?>
<database xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          name="zed" xsi:noNamespaceSchemaLocation="http://static.spryker.com/schema-01.xsd"
          namespace="Orm\Zed\PriceList\Persistence"
          package="src.Orm.Zed.PriceList.Persistence">

    <table name="fos_price_list" phpName="FosPriceList">
        <column name="id_price_list" required="true" type="INTEGER" autoIncrement="true" primaryKey="true" />
        <column name="name" required="true" size="255" type="VARCHAR"/>

        <unique name="fos_price_list-name">
            <unique-column name="name"/>
        </unique>

        <behavior name="timestampable"/>

        <id-method-parameter value="fos_price_list_pk_seq"/>
    </table>

</database>