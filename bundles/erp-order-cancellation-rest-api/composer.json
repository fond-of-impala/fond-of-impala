{
  "name": "fond-of-impala/erp-order-cancellation-rest-api",
  "description": "ERP Order Cancellation Rest Api",
  "license": "MIT",
  "authors": [
    {
      "name": "Julian Hyatt",
      "email": "dev@jzit.de"
    }
  ],
  "require": {
    "php": ">=8.1",
    "fond-of-impala/company-type": "^1.0.0",
    "fond-of-impala/erp-order-cancellation": "^1.0.0",
    "fond-of-impala/erp-order-cancellation-rest-api-extension": "^1.0.0",
    "fond-of-impala/erp-order-cancellation-extension": "^1.0.0",
    "fond-of-oryx/erp-order": "^4.8.0",
    "spryker/company": "^1.0.0",
    "spryker/company-user": "^1.0.0 || ^2.0.0",
    "spryker/glue-application": "*",
    "spryker/uuid-behavior": "^1.0.0"
  },
  "require-dev": {
    "fond-of-codeception/spryker": "*",
    "phpunit/phpunit": "<10",
    "spryker/code-sniffer": "*",
    "spryker-sdk/phpstan-spryker": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfImpala\\": "src/FondOfImpala/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfImpala\\": "tests/FondOfImpala/",
      "Generated\\": "src/Generated/",
      "Orm\\Zed\\": "src/Orm/Zed/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
