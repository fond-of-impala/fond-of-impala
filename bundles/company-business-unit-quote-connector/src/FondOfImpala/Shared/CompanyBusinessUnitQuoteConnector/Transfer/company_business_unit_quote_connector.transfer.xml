<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">
    <transfer name="CompanyBusinessUnitQuoteListRequest">
        <property name="idQuote" type="int"/>
        <property name="idCustomer" type="int"/>
        <property name="idCompanyBusinessUnit" type="int"/>
        <property name="companyUserReferences" type="string[]" singular="companyUserReference"/>
    </transfer>

    <transfer name="CompanyBusinessUnitQuoteList">
        <property name="quotes" type="Quote[]" singular="quote"/>
    </transfer>
</transfers>
