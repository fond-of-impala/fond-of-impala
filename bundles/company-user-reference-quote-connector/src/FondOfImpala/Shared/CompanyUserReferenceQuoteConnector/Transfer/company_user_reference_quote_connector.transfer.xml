<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 https://static.spryker.com/transfer-01.xsd">
    <transfer name="CompanyUserReferenceCollection">
        <property name="companyUserReferences" type="string[]" singular="companyUserReference"/>
    </transfer>

    <transfer name="CompanyUser">
        <property name="companyUserReference" type="string"/>
    </transfer>

    <transfer name="QuoteCollection">
        <property name="quotes" type="Quote[]" singular="quote"/>
    </transfer>

    <transfer name="QuoteCriteriaFilter">
        <property name="quoteIds" type="int[]"/>
    </transfer>

    <transfer name="Quote">
        <property name="companyUserReference" type="string"/>
    </transfer>

    <transfer name="QuoteResponse">
        <property name="quote" type="Quote"/>
    </transfer>
</transfers>
