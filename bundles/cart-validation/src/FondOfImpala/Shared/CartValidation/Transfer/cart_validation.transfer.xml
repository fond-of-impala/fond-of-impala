    <transfer name="RestItemsAttributes">
        <property name="validationMessages" type="Message[]" singular="validationMessage"/>
    </transfer>

    <transfer name="RestCartsAttributes">
        <property name="validationMessages" type="Message[]" singular="validationMessage"/>
    </transfer>
