<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 https://static.spryker.com/transfer-01.xsd">
    <transfer name="Quote">
        <property name="items" type="Item[]" singular="item"/>
        <property name="customer" type="Customer"/>
    </transfer>

    <transfer name="Item">
        <property name="sku" type="string"/>
        <property name="quantity" type="int"/>
        <property name="concreteDeliveryDate" type="string"/>
    </transfer>

    <transfer name="CheckoutError">
        <property name="errorType" type="string"/>
        <property name="errorCode" type="int" />
        <property name="message" type="string" />
        <property name="parameters" type="array"/>
    </transfer>

    <transfer name="CheckoutResponse">
        <property name="isSuccess" type="bool" />
        <property name="errors" type="CheckoutError[]" singular="error" />
    </transfer>

    <transfer name="ConditionalAvailabilityCriteriaFilter">
        <property name="skus" type="string[]" />
        <property name="minimumQuantity" type="int" />
        <property name="warehouseGroup" type="string" />
        <property name="channel" type="string" />
    </transfer>

    <transfer name="Customer">
        <property name="availabilityChannel" type="string" />
    </transfer>
</transfers>
