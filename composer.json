{
  "name": "fond-of-impala/fond-of-impala",
  "description": "FOND OF Impala Monorepo",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Rose",
      "email": "daniel-rose@gmx.de"
    },
    {
      "name": "JÃ³zsef Geng",
      "email": "gengjozsef86@gmail.com"
    },
    {
      "name": "Julian Hyatt",
      "email": "dev@jzit.de"
    }
  ],
  "replace": {
    "fond-of-impala/conditional-availability-extension": "*"
  },
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "fond-of-codeception/spryker": "*",
    "spryker/code-sniffer": "*",
    "spryker-sdk/phpstan-spryker": "*"
  },
  "autoload": {
    "psr-4": {
      "FondOfImpala\\Shared\\ConditionalAvailabilityExtension\\": "bundles/conditional-availability-extension/src/FondOfImpala/Shared/ConditionalAvailabilityExtension",
      "FondOfImpala\\Zed\\ConditionalAvailabilityExtension\\": "bundles/conditional-availability-extension/src/FondOfImpala/Zed/ConditionalAvailabilityExtension"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfImpala\\Shared\\ConditionalAvailabilityExtension\\": "bundles/conditional-availability-extension/tests/FondOfImpala/Shared/ConditionalAvailabilityExtension",
      "FondOfImpala\\Zed\\ConditionalAvailabilityExtension\\": "bundles/conditional-availability-extension/tests/FondOfImpala/Zed/ConditionalAvailabilityExtension",
      "Generated\\":"src/Generated/",
      "Orm\\":"src/Orm/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
