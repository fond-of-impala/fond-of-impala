  "autoload": {
    "psr-4": {
      "FondOfImpala\\Service\\ConditionalAvailability\\": "bundles/conditional-availability/src/FondOfImpala/Service/ConditionalAvailability",
      "FondOfImpala\\Shared\\ConditionalAvailability\\": "bundles/conditional-availability/src/FondOfImpala/Shared/ConditionalAvailability",
      "FondOfImpala\\Zed\\ConditionalAvailability\\": "bundles/conditional-availability/src/FondOfImpala/Zed/ConditionalAvailability",
      "FondOfImpala\\Shared\\ConditionalAvailabilityBulkApi\\": "bundles/conditional-availability-bulk-api/src/FondOfImpala/Shared/ConditionalAvailabilityBulkApi",
